@model IEnumerable<RegisterPrice>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">لیست قیمت های ثبتی</h1>

    <table class="table">
        <thead>
        <tr>
                <th scope="col">DateRegisterTime</th>
                <th scope="col">price</th>
                <th scope="col">LastTouchPrice</th>
        </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.DateRegisterTime</td>
                        <td>@item.price</td>
                        <td>@item.LastTouchPrice</td>
                    </tr>
                }
            }

        </tbody>
    </table>
</div>